<!-- Generated Using Ansuz CMS -->
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="en" xml:lang="en">
  <head>
    <title><%= title_helper %></title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta http-equiv="Content-Script-Type" content="text/javascript" />
    <meta name="description"  content="Insight Card Services" />
  	<meta name="keywords"     content="Insight, cards, services, buisiness, ruby, ruby on rails, web development, birmingham, alabama, programmers, startup software" />
    <link rel="shortcut icon" href="favicon.ico" type="image/vnd.microsoft.icon" />
    <link rel="icon"          href="favicon.ico" type="image/vnd.microsoft.icon" />
    <%= manage_ansuz_stylesheets %>
    <%= manage_ansuz_javascripts %>
	  <%= theme_stylesheet_link_tag 'ansuz_theme_birmingham_color_graphics', 'base' -%>
  </head>
  <body id='<%= params[:controller] %>' class='<%= params[:action]%>'>
    <div class='wrapper'>
      <div class='top'>
        <div class='header'>
          <img src="/themes/ansuz_theme_birmingham_color_graphics/images/header.png" alt="Header"/>
        </div>
        <div class='menu'>
          <%= render :partial => "page/top_nav" -%>
          <div class='clear'> </div>
        </div>
        <div class="leftSide">
          <div class="content left_side">
            <div class="asSeen">
              <a href="/pages/asSeenOnTv">
                <img height="53" width="75" alt="" src="/themes/ansuz_theme_birmingham_color_graphics/images/asSeenOnTV.png"/>
              </a>
            </div>
          </div>
        </div>
        <div class='serviceTabs'>
          <div class='subNav'>
          
          </div>
        </div>
        <div class='clear'></div>
        <div class='main'>
          <%= render :partial => "page/breadcrumb" if (@page && (@page != @root)) -%>
          <%= @content_for_layout -%>
          <%= render :partial => "page/child_nav" if (@page && (!@page.children.empty?)) -%>
        </div>
      </div>
    </div>
    <%= render :partial => "page/footer" -%>
    <script type='text/javascript'>
      jQuery(document).ready(function() { 
        // setup scrolling with this one JavaScript call 
        jQuery("div.scrollable").scrollable({ 
          // scrolling configuration 
          horizontal:true,  
          size:1
        });  
      });
    </script>
  </body>
</html>
